{
    "version": "1.0.0",
    "name": "madeitbelgium/chrome",
    "description": "Laravel Chrome browser provides simple browser interaction for automation.",
    "keywords": ["webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tjebbe Lievens",
            "email": "tjebbe.lievens@madeit.be"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "facebook/webdriver": "~1.0",
        "nesbot/carbon": "~1.20|2.*",
        "illuminate/console": "~5.6|6.*",
        "illuminate/support": "~5.6|6.*",
        "symfony/console": "~4.0",
        "symfony/process": "~4.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "php-coveralls/php-coveralls": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "MadeITBelgium\\Chrome\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeITBelgium\\Chrome\\ChromeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
